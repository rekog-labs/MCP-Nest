{
  "name": "@rekog/mcp-nest",
  "version": "1.6.2",
  "description": "NestJS module for creating Model Context Protocol (MCP) servers - monorepo",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rekog-labs/MCP-Nest"
  },
  "scripts": {
    "build": "npm run build --workspace=packages/mcp-nest",
    "publish": "npm publish --access public --tag alpha --workspace=packages/mcp-nest",
    "prepare": "npm run build",
    "start:playground": "npm run start:playground --workspace=packages/mcp-nest",
    "test": "npm run test --workspace=packages/mcp-nest",
    "test:watch": "npm run test:watch --workspace=packages/mcp-nest",
    "lint": "npm run lint --workspace=packages/mcp-nest",
    "format": "npm run format --workspace=packages/mcp-nest"
  }
}
